{
  "name": "Where am I?",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Where are you? In production? In homologation? In localhost? Not have doubt!",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["whereami.js"],
      "css": ["whereami.css"]
    }
  ],
  "browser_action": {
    "default_icon": "images/icon16.png",
    "popup": "popup.html"
  }
}
